<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#607D8B</item>
        <item name="colorPrimaryDark">#455A64</item>
        <item name="colorAccent">@color/ef_colorAccent</item>
    </style>

    <style name="ImagePickerTheme" parent="ef_BaseTheme">
        <item name="colorPrimary">#607D8B</item>
        <item name="colorPrimaryDark">#455A64</item>
        <item name="colorAccent">@color/ef_colorAccent</item>
    </style>

    <style name="SampleSwitch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/ef_spacing</item>
    </style>

    <style name="Poppins_Semi_Bold_Neutral80_14sp">
        <item name="android:fontFamily">@font/poppins_semi_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/neutral_80</item>
        <item name="android:includeFontPadding">false</item>
    </style>

</resources>
