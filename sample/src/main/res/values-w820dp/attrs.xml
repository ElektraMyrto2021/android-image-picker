<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="UnsplashPhotoPicker">
        <attr name="photoPicker_pageSize" format="integer"/>
        <attr name="photoPicker_spanCount" format="integer"/>
        <attr name="photoPicker_hasSearch" format="boolean"/>
        <attr name="photoPicker_persistentSearch" format="boolean"/>
        <attr name="photoPicker_isMultipleSelection" format="boolean"/>
        <attr name="photoPicker_errorDrawable" format="reference"/>
        <attr name="photoPicker_placeHolderDrawable" format="reference"/>
        <attr name="photoPicker_pickerPhotoSize" format="enum">
            <enum name="thumb" value="0"/>
            <enum name="small" value="1"/>
            <enum name="medium" value="2"/>
            <enum name="regular" value="3"/>
            <enum name="large" value="4"/>
            <enum name="full" value="5"/>
            <enum name="raw" value="6"/>
        </attr>
        <attr name="photoPicker_showPhotoSize" format="enum">
            <enum name="thumb" value="0"/>
            <enum name="small" value="1"/>
            <enum name="medium" value="2"/>
            <enum name="regular" value="3"/>
            <enum name="large" value="4"/>
            <enum name="full" value="5"/>
            <enum name="raw" value="6"/>
        </attr>
        <attr name="photoPicker_searchHint" format="string"/>
        <attr name="photoPicker_photoByString" format="string"/>
        <attr name="photoPicker_onString" format="string"/>
        <attr name="photoPicker_clickOpensPhoto" format="boolean"/>
        <attr name="photoPicker_longClickSelectsPhoto" format="boolean"/>
    </declare-styleable>

</resources>